  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with salesData as (
    select
        e.FirstName,
        e.LastName,
        c.CategoryName,
        p.ProductName,
        YEAR(o.OrderDate) as SalesYear,
        o.ShipCountry,
        SUM(od.UnitPrice * od.Quantity*(1-Discount)) as SalesPrice
	from Orders as o
    join [Order Details] as od ON o.OrderID = od.OrderID 
    join Products as p ON p.ProductID = od.ProductID 
    join Employees as e ON e.EmployeeID = o.EmployeeID 
    join Categories as c ON c.CategoryID = p.CategoryID
    group by e.FirstName, e.LastName, c.CategoryName, ProductName,o.ShipCountry, YEAR(o.OrderDate)
)
SELECT SalesYear, Firstname + LastName AS SalesName, CategoryName, ProductName, ShipCountry, SalesPrice
FROM SalesData</CommandText>
      </Query>
      <Fields>
        <Field Name="SalesYear">
          <DataField>SalesYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SalesName">
